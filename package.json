{
  "name": "hhv",
  "version": "0.1.0",
  "description": "Poker HandHistory Visualizer.",
  "main": "hhv.js",
  "scripts": {
    "test": "tap test/*.js",
    "watch": "watchify -d client/main.js -o client/bundle.js",
    "bundle-min": "browserify client/main.js | uglifyjs >client/bundle.js",
    "bundle": "browserify -d client/main.js > client/bundle.js"
  },
  "browser": {
    "./lib/templates.js": "./lib/browser-templates.js"
  },
  "browserify": {
    "transform": [
      "hbsfy"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/hhv.git"
  },
  "homepage": "https://github.com/thlorenz/hhv",
  "dependencies": {
    "handlebars": "~4.0.5",
    "hbsfy": "~2.5.1"
  },
  "devDependencies": {
    "browserify": "~13.0.0",
    "hha": "~0.1.0",
    "hhp": "~0.1.0",
    "tape": "~4.5.0",
    "uglifyjs": "~2.4.10",
    "watchify": "~3.7.0"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/hhv/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.8"
  }
}
